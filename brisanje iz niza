#include <stdio.h>
#define N 10
int main() {
	int i, pol,val,pom;
	int niz[N] = { 1,2,3,4,5,};
	int n = 6;


	//Upis
	printf("unesi polo≈æaj!\n");
	scanf("%d",&pol);
	

	//ispis
	for (i = 0; i < N; i++) {
		printf("%d,", niz[i]);
	}
	printf("\n");

	//brisanje
	for (i = pol - 1; i < n - 1; i++)
		niz[i] = niz[i+1];

	//ispis
	for (i = 0; i < N; i++) {
		printf("%d,", niz[i]);
	}

	printf("\n");


}

